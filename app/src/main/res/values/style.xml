<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--首页底部导航样式-->
    <style name="tab_menu_item">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:textColor">@drawable/tab_menu_bg</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="tab_menu_item2">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@drawable/tab_menu_bg</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="tab_menu_item3">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@drawable/tab_menu_bg</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="message_width">
        <item name="android:layout_width">48dp</item>
    </style>

    <style name="title_text" parent="title_text_size">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="title_text_size">
        <item name="android:textSize">18sp</item>
    </style>
    <!--横线-->
    <style name="henxian">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/bg_hui</item>
    </style>
    <!--竖线-->
    <style name="shuxian">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/bg_hui</item>
    </style>

    <style name="wode_bianju">
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
    </style>

    <style name="wode_bianju2">
        <item name="android:layout_height">70dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingTop">10dp</item>
    </style>
    <!--login页面的每个输入框的高度-->
    <style name="login_height">
        <item name="android:layout_height">48dp</item>
    </style>

    <style name="login_width">
        <item name="android:layout_width">60dp</item>
    </style>

    <!--个人资料的没个item的样式-->
    <style name="gerenziliao_m">
        <item name="android:background">@drawable/ripple</item>
        <item name="android:paddingRight">14dp</item>
        <item name="android:paddingLeft">14dp</item>
        <item name="android:layout_height">50dp</item>
    </style>

    <style name="edit_base">
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:paddingRight">4dp</item>
        <item name="android:textColorHint">@color/text_hui</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="btn">
        <item name="android:background">@drawable/radio_btn</item>
        <item name="android:button">@null</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:textColor">@drawable/radio_text</item>
    </style>

    <style name="layout">
        <item name="android:layout_height">40dp</item>
        <item name="android:gravity">center|start</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>

    <style name="MyDialog" parent="android:Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_buttom_out</item>
    </style>

    <style name="PopupDialog" parent="@style/MyDialog">
        <item name="android:windowAnimationStyle">@style/AnimBottom</item>
    </style>

    <style name="text_tijiao">
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/maincolor</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="layout_querendindan">
        <item name="android:layout_height">40dp</item>
        <item name="android:paddingLeft">10dp</item>
    </style>

    <style name="customTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/main_red</item>//标题栏背景色
        <item name="colorPrimaryDark">@color/main_red</item>
    </style>

    <style name="bar_back_width_bai">
        <item name="android:background">@drawable/ripple</item>
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="wodedindan_sty" parent="wodedindan">
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:textColor">@color/texthei</item>
    </style>

    <style name="bianxianhei" parent="wodedindan_sty">
        <item name="android:background">@drawable/bianxian_btn_hei_shape</item>
    </style>

    <style name="bianxianhong" parent="wodedindan_sty">
        <item name="android:background">@drawable/bianxian_red_shape</item>
    </style>

    <style name="wodedindan">
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">26dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="wode_gao">
        <item name="android:layout_height">45dp</item>
        <item name="android:gravity">center|left</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:background">@drawable/ripple</item>
    </style>

    <style name="ad">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">170dp</item>
        <item name="android:scaleType">centerCrop</item>
        <item name="android:src">@drawable/zu</item>
    </style>
    <!--bar样式-->
    <style name="bar">
        <item name="android:layout_height">48dp</item>
        <item name="android:background">@color/maincolor</item>
    </style>
    <!--bara两边的宽度-->
    <style name="bar_back_width">
        <item name="android:background">@drawable/ripple_hong</item>
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="jiantou_image">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
    </style>

    <style name="barText">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="text_yuandian">
        <item name="android:layout_gravity">right</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:background">@drawable/leirong_hong_yuan2</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
    </style>

    <!-- Navigation Header -->
    <style name="NavPage">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
        <!-- <item name="android:background">@drawable/nav_page</item> -->
        <item name="android:background">#FFE1E8EB</item>
    </style>
</resources>